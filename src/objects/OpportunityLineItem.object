    <fields>
        <fullName>Pro_Rata_Discount__c</fullName>
        <externalId>false</externalId>
        <formula>(IF(
	(PricebookEntry.Product2.E_Delivery_Training_Product__c =TRUE)
	,((E_Delivery_Algorithm_Price__c* E_Delivery_Total_Licences__c)/ Quantity)
	, PricebookEntry.UnitPrice
	)
)
-
(IF(
		(Pro_Rata_Product__c = FALSE)
		,0
		,(IF(
				(ISBLANK(PricebookEntry.Product2.Renewal_Multiplier__c))
				,(((Pro_Rata_End_Date__c - Pro_Rata_Start_Date__c)/(365.25* PricebookEntry.Product2.Duration_Years__c ))*
							(IF(
									(PricebookEntry.Product2.E_Delivery_Training_Product__c =TRUE)
									,((E_Delivery_Algorithm_Price__c* E_Delivery_Total_Licences__c)/ Quantity)
									, PricebookEntry.UnitPrice
							))
				)
				,((((Pro_Rata_End_Date__c - Pro_Rata_Start_Date__c)/(365.25* PricebookEntry.Product2.Duration_Years__c ))* PricebookEntry.Product2.Renewal_Multiplier__c*
							(IF(
								(PricebookEntry.Product2.E_Delivery_Training_Product__c =TRUE)
								,((E_Delivery_Algorithm_Price__c* E_Delivery_Total_Licences__c)/ Quantity)
								, PricebookEntry.UnitPrice
							))
				)
					+ ((1-PricebookEntry.Product2.Renewal_Multiplier__c)*
							(IF(
								(PricebookEntry.Product2.E_Delivery_Training_Product__c =TRUE)
								,((E_Delivery_Algorithm_Price__c* E_Delivery_Total_Licences__c)/ Quantity)
								, PricebookEntry.UnitPrice
							))
						)
)))))</formula>
        <label>Pro-Rata Discount</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <type>Currency</type>
    </fields>
