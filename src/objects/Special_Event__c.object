        <externalId>false</externalId>
        <formula>IF(ISBLANK(Venue__r.Principle_Address_Building__c),&quot;&quot;,Venue__r.Principle_Address_Building__c &amp; BR()) +
IF(ISBLANK(Venue__r.Principle_Address_Street__c),&quot;&quot;,Venue__r.Principle_Address_Street__c &amp; BR()) +
IF(ISBLANK(Venue__r.Principle_Address_Area__c),&quot;&quot;,Venue__r.Principle_Address_Area__c &amp; BR()) +
IF(ISBLANK(Venue__r.Principle_Address_City__c),&quot;&quot;,Venue__r.Principle_Address_City__c &amp; BR()) +
IF(ISBLANK(Venue__r.Principle_Address_State_Province__c ),&quot;&quot;,Venue__r.Principle_Address_State_Province__c &amp; BR()) +
IF(ISBLANK(Venue__r.Principle_Address_ZIP_Postal_Code__c),&quot;&quot;, Venue__r.Principle_Address_ZIP_Postal_Code__c &amp; BR()) +
IF(ISPICKVAL(Venue__r.Principle_Address_Country__c, &quot;--none--&quot;),&quot;&quot;,TEXT(Venue__r.Principle_Address_Country__c))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
