        <url>{!REQUIRESCRIPT(&apos;/soap/ajax/18.0/connection.js&apos;)}

var query = &quot;select id, name, email, phone, MobilePhone from contact where id in (select contactid from opportunityContactRole where  OpportunityId=&apos;{!Special_Event__c.Opportunity_ID__c}&apos; AND IsPrimary=true) limit 1&quot;;


var queryResults = sforce.connection.query(query);
var contacts = queryResults.getArray(&quot;records&quot;);

if( contacts.length == 1 ){
   var cName = &quot;&quot;;
   var cphone = &quot;&quot;;
   var cemail = &quot;&quot;;
   var mPhone = &quot;&quot;;

   if (contacts[0].Name!= null) {
       cName = &quot; |Contact: &quot; + contacts[0].Name;
   }
   if (contacts[0].Email!= null) {
       cemail = &quot; |Email: &quot; + contacts[0].Email ;
   }
   if (contacts[0].Phone!= null) {
       cphone = &quot; |Phone: &quot; + contacts[0].Phone;
   }
   if (contacts[0].MobilePhone!= null) {
       mPhone= &quot; |Mobile: &quot; + contacts[0].MobilePhone;
   }

   var url = &quot;a2R/e?retURL=%2Fa2R%2Fo&amp;RecordType=012D0000000JLtc&amp;ent=01I20000000TvYW&amp;CF00N20000002CVda={!Special_Event__c.Name}&amp;CF00N20000002CVda_lkid={!Special_Event__c.Id}&amp;00N20000002CVd7=&quot; + cName + &apos; &apos; + cemail + &apos; &apos; + cphone + &apos; &apos; + mPhone + &quot;&quot;;

   window.location = url;
} else {
   var url = &quot;a2R/e?retURL=%2Fa2R%2Fo&amp;RecordType=012D0000000JLtc&amp;ent=01I20000000TvYW&amp;CF00N20000002CVda={!Special_Event__c.Name}&amp;CF00N20000002CVda_lkid={!Special_Event__c.Id}&quot;;

   window.location = url;
}</url>
