    <validationRules>
        <fullName>Email_Required</fullName>
        <active>false</active>
        <description>If the Contact is not departed, deceased or the email address is unknown, then the email is required.</description>
        <errorConditionFormula>AND( 
OR( 
ISNULL(Email), 
ISBLANK(Email)), 
Departed__c = False, 
Deceased__c = False, 
Email_Not_Known__c = False, 
Active__c = True 
)</errorConditionFormula>
        <errorMessage>Email Required for this contact, please contact Events or Support if there is an issue with this.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>LID__Check_CompanyId_Value</fullName>
        <active>true</active>
        <description>Throws an exception message if the company id contains non-numerics</description>
        <errorConditionFormula>AND(
	(LID__LinkedIn_Company_Id__c &lt;&gt; NULL),
	NOT(ISNUMBER(LID__LinkedIn_Company_Id__c))
)</errorConditionFormula>
        <errorDisplayField>LID__LinkedIn_Company_Id__c</errorDisplayField>
        <errorMessage>CompanyId should be a numeric value</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>No_Cnct_Insrt_with_Accnt_DC_1_or_3_or_6</fullName>
        <active>true</active>
        <errorConditionFormula>AND (ISNEW(), OR(Account.Data_Cleanse_1__c == true, Account.Data_Cleanse_3__c == true, Account.Data_Cleanse_6__c == true))</errorConditionFormula>
        <errorMessage>You can not insert new Contact as this account will be deleted under Data Cleanse Project. Please contact IT team on 07789933270</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>first_name_salutation_required</fullName>
        <active>true</active>
        <description>Excluding administrator roles to allow initial data imports</description>
        <errorConditionFormula>AND(
 
$User.ProfileId &lt;&gt; &apos;00e20000001Mkam&apos;,
OR(
			(ISBLANK( FirstName ))
			)
)</errorConditionFormula>
        <errorDisplayField>FirstName</errorDisplayField>
        <errorMessage>First Name and Salutation needs to be Populated</errorMessage>
    </validationRules>
