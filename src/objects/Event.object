    <validationRules>
        <fullName>Assignment_Permission_Check</fullName>
        <active>true</active>
        <description>Checks that a user is able to assign task to others</description>
        <errorConditionFormula>AND(OwnerId &lt;&gt; $User.Id , $User.Task_Assign_Permitted__c = FALSE)</errorConditionFormula>
        <errorMessage>You are not permitted to assign tasks to others. Check with your administrator for details</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Stop_Partner_User_Amending_Activity</fullName>
        <active>true</active>
        <description>This will stop partner users from Amending activities that are linked to BSC Events</description>
        <errorConditionFormula>AND
(
   TEXT($User.UserType) = &apos;PowerPartner&apos;,
   OwnerId = $User.Id,
