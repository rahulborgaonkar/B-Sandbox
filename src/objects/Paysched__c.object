    <validationRules>
        <fullName>Check_Max_If_Fixed</fullName>
        <active>true</active>
        <description>checks that the Max field is greater than 0 if the Type is Fixed</description>
        <errorConditionFormula>AND(ISPICKVAL(Type_Display__c , &quot;Fixed&quot;), 
ISNULL(Max__c))</errorConditionFormula>
        <errorDisplayField>Max__c</errorDisplayField>
        <errorMessage>A Payment Schedule with a type of Fixed must have a value in the Max field</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_Max_If_Ongoing</fullName>
        <active>true</active>
        <description>checks that the Max field is blank if the type is On Going</description>
        <errorConditionFormula>AND(
ISPICKVAL( Type_Display__c , &quot;On Going&quot;), 
Max__c &gt;0)</errorConditionFormula>
        <errorDisplayField>Max__c</errorDisplayField>
        <errorMessage>A Payment Schedule with a type of On Going must have a blank Max field</errorMessage>
    </validationRules>
