    <validationRules>
        <fullName>Dimension_2_for_LMS_subs_should_be_blank</fullName>
        <active>true</active>
        <description>LMS dim 2 codes are handled in code to distinguish between New, Existing and consolidated customers.  FinanceHelper.cls.  This rule was put in to prevent confusion whereby product.dim 2 is changed but not reflected on subsequently created invoice lines</description>
        <errorConditionFormula>IF( E_Delivery_Subscription__c &amp;&amp; NOT(ISBLANK(Dimension_2__c)), true, false)</errorConditionFormula>
        <errorDisplayField>Dimension_2__c</errorDisplayField>
        <errorMessage>LMS dim 2 codes are handled in code to distinguish between New, Existing and Consolidated customers, therefore should be left blank on the product record</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Sub_Products_must_have_Dimension_2</fullName>
        <active>false</active>
        <description>All Subscription products must have a Dimension 2 code.</description>
        <errorConditionFormula>IF(Subscription_Product__c = true &amp;&amp;  ISBLANK(Dimension_2__c), true, false)</errorConditionFormula>
        <errorDisplayField>Dimension_2__c</errorDisplayField>
        <errorMessage>You cannot have a subscription product without a Dimension 2 code.</errorMessage>
    </validationRules>
