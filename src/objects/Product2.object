        <externalId>false</externalId>
        <formula>IF (TEXT(Duration__c) = &quot;1 Year&quot;,1,
(IF (TEXT(Duration__c) = &quot;2 Years&quot;,2,
(IF (TEXT(Duration__c) = &quot;3 Years&quot;,3,
(IF (TEXT(Duration__c) = &quot;100 Years&quot;,100,
0)))))))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
