    <validationRules>
        <fullName>Close_Date</fullName>
        <active>true</active>
        <description>Ensures that if the user doesn&apos;t have the field &quot;Manage Opportunity Closure&quot; checked, the close date cannot be set to before the current date</description>
        <errorConditionFormula>AND(
	AND(
		OR(
			ISNEW(), 
			ISCHANGED( CloseDate )
			), 
		(CloseDate &lt;  TODAY())
