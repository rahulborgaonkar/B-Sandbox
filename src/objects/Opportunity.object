        <fullName>Credit_Latest_Invoice</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Credit Latest Invoice</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/26.0/connection.js&quot;)} 
{!REQUIRESCRIPT(&quot;/soap/ajax/26.0/apex.js&quot;)}  

if (&apos;{!Opportunity.Using_one_to_one_invoicing__c}&apos; == &apos;Yes&apos; &amp;&amp;
    &apos;{!Opportunity.Finance_to_Credit_Opp__c}&apos; == &apos;1&apos; &amp;&amp;
    &apos;{!Opportunity.Invoice_Status__c}&apos; == &apos;To Be Credited&apos; &amp;&amp;
    &apos;{!Opportunity.Latest_InvoiceId__c}&apos; != &apos;&apos;){

   var compCorrect = sforce.apex.execute(&quot;FinanceHelper&quot;, &quot;usrInCorrectFFCompany&quot;, {company:&quot;{!Opportunity.Invoice_Company__c}&quot;}); 
   if (&apos;{!Opportunity.Invoice_Company__c}&apos; != &apos;&apos; &amp;&amp; compCorrect == &apos;false&apos;){ 
      alert(&apos;You are trying to create an invoice for the company {!Opportunity.Invoice_Company__c} but you dont have this company selected&apos;); 
      
   } else  {
      alert(&apos;Creating Credit Note.&apos;); 
      sforce.apex.execute(&quot;AutoInvoiceHelper&quot;, &quot;wsCreateCreditNoteFromOpp&quot;, {invoiceId:&quot;{!Opportunity.Latest_InvoiceId__c}&quot;,invoiceCompany:&quot;{!Opportunity.Invoice_Company__c}&quot;}); 
      alert(&apos;Credit Note Created.&apos;); 
      document.location.reload(true); 
   }
} else {
   if (&apos;{!Opportunity.Using_one_to_one_invoicing__c}&apos; == &apos;No&apos;)
      alert(&apos;Old world invoices cannot be credited using this process. Please credit this opportunity using old method&apos;);
   else
      alert(&apos;Cannot create credit note, Either a Latest Invoice does not exist or Finance to Credit Opp is not checked.&apos;);
}</url>
