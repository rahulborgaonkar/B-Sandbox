        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!requireScript(&quot;/soap/ajax/20.0/connection.js&quot;)} 
{!requireScript(&quot;/soap/ajax/20.0/apex.js&quot;)} 

if(&apos;{!$Profile.Name}&apos; == &apos;A1 Logistics Baseline&apos; || &apos;{!$Profile.Name}&apos; == &apos;System Administrator&apos;)
{
if(&apos;{!Opportunity.StageName}&apos; == &apos;Booking Received&apos; || &apos;{!Opportunity.StageName}&apos; == &apos;Paid&apos; || &apos;{!Opportunity.StageName}&apos; == &apos;Invoiced&apos;) 
{
	if(&apos;{!Opportunity.Joining_Instructions_Sent__c}&apos; != &apos;1&apos;)
	{

		if(&apos;{!Opportunity.RecordType}&apos; == &apos;Public Event&apos; || &apos;{!Opportunity.RecordType}&apos; == &apos;In-Company Event&apos;)
		{
			var ret = sforce.apex.execute(&quot;OpportunityJoiningInstructionMethods&quot;, &quot;sendJoiningInstructions&quot;, {oppId:&apos;{!Opportunity.Id}&apos;, oppRecType:&apos;{!Opportunity.RecordType}&apos;});
			if(ret == &apos;&apos;)
			{
				alert(&apos;Joining Instructions has sent successfully&apos;);
			}
			else
			{
				alert(&apos;ERROR : &apos; + ret);
			}

		}
		else
		{
			alert(&apos;ERROR : Opportunity Record Type must be Public Event or In-Company Event&apos;);
		}
	}
	else
	{
		alert(&apos;ERROR : Joining Instruction has been sent for this Opportunity&apos;);
	}
}
else
{
	alert(&apos;ERROR : Opportunity Stage should be Booking Recieved to Send Joining Instructions&apos;);
}
}
else
{
alert(&apos;ERROR : This button is used by Operations team only&apos;);
}</url>
