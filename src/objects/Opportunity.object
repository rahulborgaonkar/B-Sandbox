    <webLinks>
        <fullName>Update_Addresses_from_Account</fullName>
        <availability>online</availability>
        <description>Updates the billing and principle address on the opportunity using the address on the Account.</description>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Update Addresses from Account</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/18.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/18.0/apex.js&quot;)}

var url = parent.location.href;


if (confirm(&apos;Are you sure you want to overwrite the opportunity addresses with the default account addresses?&apos;)){
var result = sforce.connection.query(&quot;Select Name, BillingStreet, BillingState, BillingPostalCode, BillingCountry, BillingCity, Principle_Address_Area__c, Principle_Address_Building__c, Principle_Address_City__c, Principle_Address_Country__c, Principle_Address_Postcode_Area_UK__c, Principle_Address_State_Province__c, Principle_Address_Street__c, Principle_Address_ZIP_Postal_Code__c FROM Account WHERE Id = &apos;{!Opportunity.AccountId}&apos; LIMIT 1&quot;);

acc = result.getArray(&quot;records&quot;);

var update_opp = new sforce.SObject(&quot;Opportunity&quot;); 

update_opp.Id = &apos;{!Opportunity.Id}&apos;;
update_opp.Billing_State__c = acc[0].BillingState;
update_opp.Billing_Address_Account_Name__c = acc[0].Name;
update_opp.Billing_Street__c = acc[0].BillingStreet;
update_opp.Billing_PostalCode__c = acc[0].BillingPostalCode;
update_opp.Billing_Country__c = acc[0].BillingCountry;
update_opp.Billing_City__c = acc[0].BillingCity;

update_opp.Shipping_Address_Account_Name__c = acc[0].Name;
update_opp.Shipping_Area__c = acc[0].Principle_Address_Area__c;
update_opp.Shipping_Address_Building__c = acc[0].Principle_Address_Building__c;
update_opp.Shipping_City__c = acc[0].Principle_Address_City__c;
update_opp.Shipping_Country__c = acc[0].Principle_Address_Country__c;
update_opp.Shipping_State__c = acc[0].Principle_Address_State_Province__c;
update_opp.Shipping_Street__c = acc[0].Principle_Address_Street__c;
update_opp.Shipping_Postal_Code__c = acc[0].Principle_Address_ZIP_Postal_Code__c;

result = sforce.connection.update([update_opp]);

if (result){
  alert(&apos;Addresses copied from Opportunity Account to the Opportunity successfully&apos;);
  parent.location.href = url; 
}
}</url>
