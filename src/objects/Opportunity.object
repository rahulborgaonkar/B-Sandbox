        <url>{!REQUIRESCRIPT(&apos;/soap/ajax/18.0/connection.js&apos;)} 

var oppOwner = sforce.connection.query(&quot;Select id,IsActive FROM User WHERE Id = &apos;{!Opportunity.OwnerId}&apos; LIMIT 1&quot;);

if (oppOwner.records.IsActive == &quot;false&quot;)
    alert(&apos; Opportunity owner is In-Active, Please change to a active owner and try again&apos;);
else {
    if (&apos;{!Opportunity.StageName}&apos; == &apos;Qualifying&apos; || &apos;{!Opportunity.StageName}&apos; == &apos;Preparation&apos; || &apos;{!Opportunity.StageName}&apos; == &apos;Quote/Proposal&apos; || &apos;{!Opportunity.StageName}&apos; == &apos;Negotiation&apos; || &apos;{!Opportunity.StageName}&apos; == &apos;Invoiced&apos; || &apos;{!Opportunity.StageName}&apos; == &apos;Part Invoiced&apos; || &apos;{!Opportunity.StageName}&apos; == &apos;Paid&apos; || &apos;{!Opportunity.StageName}&apos; == &apos;Part Paid&apos;) {

        if (&apos;{!Opportunity.Invoice_Status__c}&apos; == &apos;Invoiced&apos; ||
            &apos;{!Opportunity.Invoice_Status__c}&apos; == &apos;Paid&apos; ||
            &apos;{!Opportunity.Invoice_Status__c}&apos; == &apos;Part Paid&apos; ||
            &apos;{!Opportunity.Invoice_Status__c}&apos; == &apos;To Be Credited&apos; ||
            &apos;{!Opportunity.StageName}&apos; == &apos;Invoiced&apos; ||
            &apos;{!Opportunity.StageName}&apos; == &apos;Part Invoiced&apos; ||
            &apos;{!Opportunity.StageName}&apos; == &apos;Paid&apos; ||
            &apos;{!Opportunity.StageName}&apos; == &apos;Part Paid&apos;) {

            alert(&quot;This opportunity has an invoice associated with it which first needs to be credited by Finance.  Please tick the Finance To Credit Opp flag on this opportunity.  You will need to wait for the opportunity to be credited before continuing&quot;);

        } else {
            if (&apos;{!Opportunity.RecordType}&apos; == &apos;In-Company Event&apos;) {
                window.location = &apos;/apex/tncInCompanyCancellation?id={!Opportunity.Id}&amp;Object=Opportunity&apos;;
            } else if (&apos;{!Opportunity.RecordType}&apos; == &apos;Public Event&apos;) {
                window.location = &apos;/apex/tncPublicEventCancellation?oppId={!Opportunity.Id}&apos;;
            }
        }
    } else {
        alert(&quot;Please move the Opportunity stage back to Negotiation before Cancelling the Event / Booking&quot;);
    }
}</url>
