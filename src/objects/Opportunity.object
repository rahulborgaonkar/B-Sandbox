    <fields>
        <fullName>Available_For_Invoice__c</fullName>
        <description>If latest Invoice in Blank or latest invoice is populated but credited the opp can be invoiced</description>
        <externalId>false</externalId>
        <formula>IF( 
   OR(
       ISBLANK(Latest_Invoice__c),
       Latest_Invoice__r.Is_Credited_In_Full__c = true,
       AND(
           TEXT(Latest_Invoice__r.c2g__InvoiceStatus__c) = &apos;Discarded&apos;,
           TEXT( Latest_Invoice__r.c2g__InvoiceStatus__c) &lt;&gt; &apos;In Progress&apos;,
           TEXT( Latest_Invoice__r.c2g__InvoiceStatus__c) &lt;&gt; &apos;Complete&apos;
       )
   ),
   TRUE, 
   FALSE
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Available For Invoice</label>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
