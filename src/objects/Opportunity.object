    <validationRules>
        <fullName>Card_number_has_15_or_16_digits</fullName>
        <active>true</active>
        <description>The credit/debit card must have 15 digits for Amex and 16 for all other cards.</description>
        <errorConditionFormula>IF( 
   OR(
      AND(
         LEN(Card_Number__c) != 0,
         ISPICKVAL(Card_Type__c, &apos;Amex&apos;),
         LEN(Card_Number__c) !=15
      ),
      AND(
         LEN(Card_Number__c) != 0,
         NOT(ISPICKVAL(Card_Type__c, &apos;Amex&apos;)),
         LEN(Card_Number__c) !=16
      )
