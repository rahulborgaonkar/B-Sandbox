    <validationRules>
        <fullName>Cannot_Delete_or_create_opportunity_prod</fullName>
        <active>true</active>
        <description>Validation to stop any of the opportunity product lines from being deleted or created when the opportunity stage is either customer Approved, lost or is a won stage. This excludes the Website User and System Administrators</description>
        <errorConditionFormula>OR( 
AND( 
OR( 
ISPICKVAL( StageName , &quot;Customer Approved&quot;), 
ISPICKVAL( StageName , &quot;Lost&quot;), 
IsWon = TRUE), 
Number_of_Opportunity_Products__c &gt; PRIORVALUE( Number_of_Opportunity_Products__c ) , 
Transfer_Cancellation__c = FALSE, 
$User.ProfileId &lt;&gt; &quot;00e20000001Mkam&quot;, 
$User.ProfileId &lt;&gt; &quot;00eD0000001PLci&quot; 
), 
AND( 
ISPICKVAL( StageName , &quot;Lost&quot;), 
Number_of_Opportunity_Products__c &gt; PRIORVALUE( Number_of_Opportunity_Products__c ), 
$User.ProfileId &lt;&gt; &quot;00e20000001Mkam&quot;, 
$User.ProfileId &lt;&gt; &quot;00eD0000001PLci&quot; ,
Transfer_Cancellation__c = FALSE 
), 
AND( 
OR( 
ISPICKVAL( StageName , &quot;Customer Approved&quot;), 
ISPICKVAL( StageName , &quot;Lost&quot;), 
IsWon = TRUE), 
Number_of_Opportunity_Products__c &lt; PRIORVALUE( Number_of_Opportunity_Products__c ) , 
$User.ProfileId &lt;&gt; &quot;00e20000001Mkam&quot;, 
$User.ProfileId &lt;&gt; &quot;00eD0000001PLci&quot; ,
Transfer_Cancellation__c = FALSE 
))</errorConditionFormula>
        <errorMessage>You cannot delete or create opportunity product lines for opportunities with a stage of customer approved, lost or where the opportunity is won!</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cant_Req_credit_approval_without_reasons</fullName>
        <active>false</active>
        <description>Prevents user submitting a credit request without required information</description>
        <errorConditionFormula>AND ( 
Request_Credit_Approval__c = True, 
     OR
        (
           ISBLANK(TEXT(Credit_Note_Reason__c )),
           ISBLANK(TEXT(Credit_Note_Product_Group__c)),
           ISBLANK(TEXT(Product_Delivered__c))
        )
    )</errorConditionFormula>
        <errorDisplayField>Request_Credit_Approval__c</errorDisplayField>
        <errorMessage>You cannot request a credit note approval without entering Credit note reason, credit note product group and Product Delivered.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cant_Req_credit_approval_wo_reasons_new</fullName>
        <active>true</active>
        <description>Prevents user submitting a credit request without required information</description>
        <errorConditionFormula>IF
(
AND
(
	AND 
	(
		Request_Credit_Approval__c = True, 

		OR
		(
			OR
			( 
				ISBLANK(
					TEXT(Credit_Note_Reason__c )
					), 
				ISBLANK(
					TEXT(Credit_Note_Product_Group__c)
					), 
				ISBLANK(
					TEXT(Product_Delivered__c)
					) 
			),
			AND
			(
				Using_one_to_one_invoicing__c = &apos;Yes&apos;, 
				OR
				(
					AND ( 
						Finance_to_Invoice_Opp__c = False, 
						ISBLANK( Latest_Invoice__c ) 
					), 
					AND( 
						Finance_to_Invoice_Opp__c = False, 
						!ISBLANK( Latest_Invoice__c ), 
						Latest_Invoice__r.Is_Credited_In_Full__c = True, 
						!ISPICKVAL(Latest_Invoice__r.c2g__InvoiceStatus__c, &apos;Discarded&apos;) 
					),
					AND ( 
						Finance_to_Invoice_Opp__c = False, 
						!ISBLANK( Latest_Invoice__c ), 
						ISPICKVAL(Latest_Invoice__r.c2g__InvoiceStatus__c, &apos;In Progress&apos;) 
					), 
					AND ( 
						Finance_to_Invoice_Opp__c = False, 
						ISPICKVAL(Latest_Invoice__r.c2g__InvoiceStatus__c, &apos;Discarded&apos;) 
					), 
					AND ( 
						Finance_to_Invoice_Opp__c = True, 
						ISPICKVAL(Latest_Invoice__r.c2g__InvoiceStatus__c, &apos;Discarded&apos;) 
					) 
				)
			)
		) 
	)
),
TRUE,
FALSE
)</errorConditionFormula>
        <errorDisplayField>Request_Credit_Approval__c</errorDisplayField>
        <errorMessage>You cannot request a credit note approval if any of the following is true:
Credit note reason, credit note product group and Product Delivered are blank.  
There is no latest invoicee which has not already been credited or is a proforma</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cant_credit_a_opp_when_inv_in_progress</fullName>
        <active>true</active>
        <description>Stop people clicking &quot;Finance to Credit Invoice&quot; when the invoice is at &quot;In Progress&quot;.</description>
        <errorConditionFormula>AND (
   Using_one_to_one_invoicing__c = &apos;Yes&apos;,
   Finance_to_Credit_Opp__c = True,
   NOT(ISBLANK( Latest_Invoice__c)),
   ISPICKVAL(Latest_Invoice__r.c2g__InvoiceStatus__c, &apos;In Progress&apos;)
)</errorConditionFormula>
        <errorDisplayField>Finance_to_Credit_Opp__c</errorDisplayField>
        <errorMessage>There is no need to ask Finance to credit the invoice as only a proforma invoice exists (an unposted invoice).</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cant_tick_fin_to_inv_credit_if_credite</fullName>
        <active>true</active>
        <description>Stops people from ticking both the &quot;Finance to Credit Opp&quot; and &quot;Finance to Invoice Opp&quot; if the opportunity has been credited.</description>
