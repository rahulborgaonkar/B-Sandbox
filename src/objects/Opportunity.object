        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/18.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/18.0/apex.js&quot;)}

var error = false;

// *** Check Tax Code has been entered
if (&apos;{!Opportunity.Tax_Code__c}&apos; == &apos;&apos;){
     alert(&apos;ERROR Cannot continue. The Tax Code on the opportunity is blank.&apos;);
     error = true;
}

// *** Check Account is active
if (error == false){
   var acc = sforce.connection.query(&quot;Select Active__c FROM Account WHERE Id = &apos;{!Opportunity.AccountId}&apos; LIMIT 1&quot;);
   if (acc.records.Active__c == &apos;false&apos;){
       alert(&apos;ERROR The opportunity account is inactive. Check the inactive reason on the account.&apos;);
       error = true;
   }
}

// *** Check at least one product exists on the opportunity.
if (error == false){
   var oppLine = sforce.connection.query(&quot;Select Id FROM OpportunityLineItem WHERE OpportunityId = &apos;{!Opportunity.Id}&apos; LIMIT 1&quot;);
   if (oppLine.records == null){
        alert(&apos;ERROR The opportunity doesn\&apos;t have any products&apos;);
        error = true;

   }
}

// *** Check to see if there are any products on the opp that have NO EVENT set (ie no bookings are required)

if (error == false){   
   var oppLine = sforce.connection.query(&quot;Select Id FROM OpportunityLineItem o WHERE OpportunityId = &apos;{!Opportunity.Id}&apos; AND o.PriceBookEntry.Product2.No_Event__c = True LIMIT 1&quot;);

   if (oppLine.records == null){

      // *** Check at least one Booking exists on the opportunity.
      if (error == false){
         var oppLine = sforce.connection.query(&quot;Select Id FROM Special_Event_Booking__c WHERE Opportunity__c = &apos;{!Opportunity.Id}&apos; LIMIT 1&quot;);
         if (oppLine.records == null){
              alert(&apos;ERROR The opportunity doesn\&apos;t have any bookings on it&apos;);
              error = true;
         }
      }
   }
}


// *** Launch booking form if no errors have been detected
if (error == false){
     window.open(&apos;/apex/EventApplicationForm?id={!Opportunity.Id}&apos;,&apos;mywindow&apos;);
}</url>
