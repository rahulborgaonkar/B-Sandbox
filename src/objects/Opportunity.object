    <webLinks>
        <fullName>Create_Subscription</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Create Subscription</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&apos;/soap/ajax/18.0/connection.js&apos;)}

var error = false;

// *** Check Account is active
if (error == false){
   var acc = sforce.connection.query(&quot;Select Active__c FROM Account WHERE Id = &apos;{!Opportunity.AccountId}&apos; LIMIT 1&quot;);
   if (acc.records.Active__c == &apos;false&apos;){
       alert(&apos;ERROR The opportunity account is inactive. Check the inactive reason on the account.&apos;);
       error = true;
   }
}

// *** Check at least one product exists on the opportunity.
if (error == false){
   var oppLine = sforce.connection.query(&quot;Select Id FROM OpportunityLineItem WHERE OpportunityId = &apos;{!Opportunity.Id}&apos; AND PricebookEntry.Product2.Core_Subscription_Product__c = true LIMIT 1&quot;);
   if (oppLine.records == null){
        alert(&apos;ERROR The opportunity doesn\&apos;t have any core products on it&apos;);
        error = true;

   }
}

// *** Check to make sure there is a primary contact role
if (error == false){
   var oppLine = sforce.connection.query(&quot;Select Id FROM OpportunityContactRole WHERE OpportunityId = &apos;{!Opportunity.Id}&apos; AND IsPrimary = true LIMIT 1&quot;);
   if (oppLine.records == null){
        alert(&apos;ERROR The opportunity doesn\&apos;t have a primary contact role&apos;);
        error = true;

   }
}

// *** Check to see if there are subscriptions on the opportunity.
if (error == false){
   var subs = sforce.connection.query(&quot;Select Subscription__c FROM Opportunity WHERE Id = &apos;{!Opportunity.Id}&apos; AND Subscription__c !=null LIMIT 1&quot;);
   if (subs.records != null){
        alert(&apos;ERROR: This opportunity already has a subscription set on it, you can\&apos;t create a new subscription with this set.&apos;);
        error = true;
   }
}

// *** Check to see if there are subscriptions on the opportunity.
if (error == false){
   var subs = sforce.connection.query(&quot;Select Id FROM Subscription__c WHERE Opportunity__c = &apos;{!Opportunity.Id}&apos; LIMIT 1&quot;);
   if (subs.records != null){
        alert(&apos;ERROR: This opportunity already has a subscription linked to it, you can\&apos;t create a new subscription with this set.&apos;);
        error = true;
   }
}

if(&apos;{!Opportunity.Subscription_Start_Date__c}&apos; == &apos;&apos;){
   alert(&apos;ERROR: Subscription Start date cannot be empty on the opportunity. Set the start date and retry subscription creation.&apos;);
   error = true;
}

if(error == false){
   if(confirm(&apos;This will create a new subscription on this account &amp; opportunity. Are you sure?&apos;)){
       window.location=&apos;/apex/CreateSubscriptionFromOpportunity?id={!Opportunity.Id}&apos;;
   }
}</url>
