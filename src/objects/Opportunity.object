    <validationRules>
        <fullName>Debit_Credit_card_details_correct</fullName>
        <active>true</active>
        <description>Checks to make sure that all the credit/debit card details have been filled out.</description>
        <errorConditionFormula>IF(
   !ISPICKVAL(StageName,&quot;Paid&quot;), 
IF(
   OR(
      ISCHANGED(Card_Type__c),
      ISCHANGED(Name_on_Card__c),
      ISCHANGED(CV2__c),
      ISCHANGED(Credit_Card_Email__c),
      ISCHANGED(Credit_Card_Amount__c),
      ISCHANGED(Expiry_Date__c),
      ISCHANGED(Card_Number__c),
      ISNEW()
   ),
   IF(
      OR(
         AND(
            !ISBLANK(TEXT(Card_Type__c)),
            OR(
               ISBLANK(Name_on_Card__c), 
               LEN(Card_Number__c) = 0, 
               ISBLANK(CV2__c), 
               ISBLANK(Credit_Card_Email__c), 
               ISBLANK(Credit_Card_Amount__c), 
               Credit_Card_Amount__c = 0, 
               ISBLANK(Expiry_Date__c)
            )
         ),
         AND(
            LEN(Card_Number__c) &gt; 0,
            OR(
               ISBLANK(Name_on_Card__c), 
               ISBLANK(CV2__c), 
               ISBLANK(TEXT(Card_Type__c)), 
               ISBLANK(Credit_Card_Email__c), 
               ISBLANK(Credit_Card_Amount__c), 
               Credit_Card_Amount__c = 0, 
               ISBLANK(Expiry_Date__c) 
            )
         ) 
      ),
      TRUE,FALSE
   ),FALSE
),FALSE
)</errorConditionFormula>
        <errorMessage>One of the following fields has missing information: &apos;Name on Card&apos;, &apos;Card Number&apos;, &apos;CV2&apos;, &apos;Expiry Date&apos;,&apos;Card Holder Contact&apos;, &apos;Credit Card Email&apos;,&apos;Card Type&apos; or &apos;Credit Card Amount&apos;. Please enter and try again</errorMessage>
    </validationRules>
