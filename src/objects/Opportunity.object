        <fullName>Generate_Invoice</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Generate Invoice</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/26.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/26.0/apex.js&quot;)}
// 27/02: changed above api from 18 to 26

var error = false;

var useOneToOne = sforce.apex.execute(&quot;FinanceHelper&quot;, &quot;useOneToOneInvoicing&quot;, {sObject:&quot;{!Opportunity.Id}&quot;});

var compCorrect = sforce.apex.execute(&quot;FinanceHelper&quot;, &quot;usrInCorrectFFCompany&quot;, {company:&quot;{!Opportunity.Invoice_Company__c}&quot;});

var acc = sforce.connection.query(&quot;Select c2g__CODAOutputVATCode__c, c2g__CODATaxCalculationMethod__c FROM Account WHERE Id = &apos;{!Opportunity.AccountId}&apos; LIMIT 1&quot;);

var taxCode = acc.records.c2g__CODAOutputVATCode__c;
if (taxCode == null){
   alert(&apos;ERROR: Account does not have a valid Tax Code&apos;);
   //error = true;
} else{
   taxCode = taxCode.substring(0,15);
}

var calcMethod = acc.records.c2g__CODATaxCalculationMethod__c;
if (calcMethod== null){
   alert(&apos;ERROR: Account does not have a Tax Calculation Method set.&apos;);
   //error = true;
} 

if (error == false &amp;&amp; {!Opportunity.Proforma_exists__c} == 1 &amp;&amp; useOneToOne == &apos;false&apos;){
   if (confirm(&apos;WARNING: A proforma invoice already exists for this opportunity, Do you wish to continue?&apos;)){
      error = false;
   }
}

if (error == false &amp;&amp; {!Opportunity.Proforma_exists__c} == 1 &amp;&amp; useOneToOne == &apos;true&apos;){
   alert(&apos;A proforma invoice already exists for this opportunity, you must either discard the invoice (if details are different) or post it&apos;);
   error = true;
}

if (error == false &amp;&amp; &apos;{!Opportunity.Invoice_Company__c}&apos; != &apos;&apos; &amp;&amp; compCorrect == &apos;false&apos;){
   alert(&apos;You are trying to create an invoice for the company {!Opportunity.Invoice_Company__c} but you dont have this company selected&apos;);
   error = true;
}

if (error == false &amp;&amp; {!Account.PO_Number_always_required_on_orders__c} ==1 &amp;&amp; &quot;{!Opportunity.Purchase_Order_Reference__c}&quot; == &quot;&quot;){
   alert(&apos;A purchase order reference is required for this account. Please pass back to the owner of the opportunity to add&apos;);
   error = true;
}

if (error == false &amp;&amp; useOneToOne == &apos;true&apos;){
   if (&apos;{!Opportunity.Invoice_Status__c}&apos; == &apos;Invoiced&apos; || &apos;{!Opportunity.Invoice_Status__c}&apos; == &apos;Paid&apos; || &apos;{!Opportunity.Invoice_Status__c}&apos; == &apos;Part Paid&apos;){
      error = true;
      alert(&quot;You can only invoice an opportunity that has not already been invoiced, paid or part paid&quot;);
   }
}

if (error == false &amp;&amp; useOneToOne == &apos;true&apos;){
   if ({!Opportunity.Finance_to_Invoice_Opp__c} == 0){
      error = true;
      alert(&quot;Opportunity doesn&apos;t have &apos;Finance to Invoice Opp&apos; ticked. This must be ticked before the invoice is created&quot;);
   }
}

if (error == false &amp;&amp; useOneToOne == &apos;true&apos; &amp;&amp; &apos;{!Opportunity.Invoice_Status__c}&apos; == &apos;No Action&apos; &amp;&amp; &apos;{!Opportunity.Finance_to_Invoice_Opp__c}&apos; == &apos;1&apos;) {

   error = true;
   alert(&quot;The invoice status on the opportunity states that an invoice is not to be created at this time. There may be a number of reasons for this, speak to IT for more information.&quot;);

}

if (error == false &amp;&amp; useOneToOne == &apos;true&apos;){
   if (&apos;{!Opportunity.IsWon}&apos; ==&apos;1&apos; &amp;&amp; (&apos;{!Opportunity.Invoice_Status__c}&apos; == &apos;Credited&apos; || &apos;{!Opportunity.Invoice_Status__c}&apos; == &apos;To Be Invoiced&apos;) ){

   }else{
      error = true;
      alert(&quot;You can only invoice an opportunity with a stage at &apos;Booking Received&apos; or if the stage is &apos;Invoiced&apos; and the opportunity has been credited or &apos;To Be Invoiced&apos;. &quot;);
   }
}

if (error == false){
   var bookings = sforce.connection.query(&quot;SELECT Id FROM Special_Event_Booking__c WHERE Opportunity__c = &apos;{!Opportunity.Id}&apos; AND Status__c = &apos;Provisional&apos; LIMIT 1&quot;);

   if (bookings.records !=null){
      alert(&quot;This opportunity still has bookings that are provisional.  Please contact the opportunity owner to rectify&quot;);
      error = true;
   }
}

if (error == false){

   if(&apos;{!Opportunity.IsWon}&apos; !=&apos;1&apos;) {
        alert(&quot;Opportunity needs to be Won&quot; );
   } else {
       if (acc.records != null){

           if(&apos;{!Opportunity.Tax_CodeId__c}&apos; == taxCode){
               if(useOneToOne == &apos;true&apos;){
                   //document.location.href = &quot;/apex/GenerateInvoice?id={!Opportunity.Id}&amp;use121=true&quot;;
                   alert(&apos;Creating Invoice.&apos;);
                   sforce.apex.execute(&quot;AutoInvoiceHelper&quot;, &quot;wsCreateInvoiceFromOpp&quot;, {oppId:&quot;{!Opportunity.Id}&quot;,invoiceCompany:&quot;{!Opportunity.Invoice_Company__c}&quot;});
                   alert(&apos;Invoice Created.&apos;);
                   document.location.reload(true);
               }else{
                   document.location.href = &quot;/apex/GenerateInvoice?id={!Opportunity.Id}&amp;use121=false&quot;;
               }
           }else{
                alert(&quot;The Opportunity Owner has chosen the tax code &apos;{!Opportunity.Tax_Code__c}&apos; which is different to the one on the account. Make sure you change the tax code on the invoice!&quot;);
               if(useOneToOne == &apos;true&apos;){
                   //document.location.href = &quot;/apex/GenerateInvoice?id={!Opportunity.Id}&amp;use121=true&quot;;
                   alert(&apos;Creating Invoice.&apos;);
                   sforce.apex.execute(&quot;AutoInvoiceHelper&quot;, &quot;wsCreateInvoiceFromOpp&quot;, {oppId:&quot;{!Opportunity.Id}&quot;,invoiceCompany:&quot;{!Opportunity.Invoice_Company__c}&quot;});
                   alert(&apos;Invoice Created.&apos;);
                   document.location.reload(true);
               }else{
                   document.location.href = &quot;/apex/GenerateInvoice?id={!Opportunity.Id}&amp;use121=false&quot;;
               }
           }
       } else {
           alert(&quot;Opportunity does not have an account&quot;);
       }
   }
}</url>
