    <validationRules>
        <fullName>Mandatory_if_Final_Proposal</fullName>
        <active>true</active>
        <description>Where the Status of the project is from the value &quot;Final Proposal&quot; then make this field mandatory.</description>
        <errorConditionFormula>AND(
OR (
ISPICKVAL(Status__c, &quot;Final Proposal&quot;),
ISPICKVAL(Status__c, &quot;Negotiating Terms&quot;),
ISPICKVAL(Status__c, &quot;Pilot/Taster Session Booked&quot;),
ISPICKVAL(Status__c, &quot;Project Ongoing&quot;), 
ISPICKVAL(Status__c, &quot;Project Complete&quot;)),
ISPICKVAL(Standard_T_Cs__c,&quot;&quot;))</errorConditionFormula>
        <errorDisplayField>Standard_T_Cs__c</errorDisplayField>
        <errorMessage>Please enter a Standard T&amp;Cs</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Mandatory_if_Initia_Proposal_Bus_Dev_Mgr</fullName>
        <active>true</active>
        <description>If the Status is from Final Proposal, this field should be mandatory</description>
        <errorConditionFormula>AND(
OR (
ISPICKVAL(Status__c, &quot;Initial Proposal&quot;),
ISPICKVAL(Status__c, &quot;Final Proposal&quot;),
ISPICKVAL(Status__c, &quot;Negotiating Terms&quot;),
ISPICKVAL(Status__c, &quot;Pilot/Taster Session Booked&quot;),
ISPICKVAL(Status__c, &quot;Project Ongoing&quot;),
ISPICKVAL(Status__c, &quot;Project Complete&quot;)),
ISBLANK( BSC_Business_Development_Manager__c ))</errorConditionFormula>
        <errorDisplayField>BSC_Business_Development_Manager__c</errorDisplayField>
        <errorMessage>Please select the BSC Business Development Manager</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Mandatory_if_Initia_Proposal_JOMC_Del_Ld</fullName>
        <active>true</active>
        <description>If the Status is from Initial Proposal, this field should be mandatory</description>
        <errorConditionFormula>AND(
OR (
ISPICKVAL(Status__c, &quot;Initial Proposal&quot;),
ISPICKVAL(Status__c, &quot;Final Proposal&quot;),
ISPICKVAL(Status__c, &quot;Negotiating Terms&quot;),
ISPICKVAL(Status__c, &quot;Pilot/Taster Session Booked&quot;),
ISPICKVAL(Status__c, &quot;Project Ongoing&quot;),
ISPICKVAL(Status__c, &quot;Project Complete&quot;)),
 ISBLANK(JOMC_Delivery_Lead__c))</errorConditionFormula>
        <errorDisplayField>JOMC_Delivery_Lead__c</errorDisplayField>
        <errorMessage>Please select the JOMC Delivery Lead</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Mandatory_if_Standard_T_C_is_No</fullName>
        <active>true</active>
        <description>If the value of Standard T&amp;Cs is No,  the T&amp;C Amendments is mandatory.</description>
        <errorConditionFormula>AND(
 ISPICKVAL( Standard_T_Cs__c , &quot;No&quot;) ,
 T_C_Amendements__c =&quot;&quot;)</errorConditionFormula>
        <errorDisplayField>T_C_Amendements__c</errorDisplayField>
        <errorMessage>If the value of Standard T&amp;Cs is No, please enter the T&amp;C Amendments</errorMessage>
    </validationRules>
