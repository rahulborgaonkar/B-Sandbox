        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/18.0/connection.js&quot;)}

var status = &apos;{!Special_Event_Booking__c.Status__c}&apos;;
var type = &apos;{!Special_Event_Booking__c.Type__c }&apos;;


if(&apos;{!Special_Event_Booking__c.Transferred_To__c}&apos;!==&apos;&apos;)
{
    alert(&quot;You cannot transfer an already transferred booking&quot;);
}
else
{

   if(type != &apos;Public&apos;)
   {
       alert(&quot;Must be a public booking.&quot;);
   }
   else{
      if( status == &apos;To Be Transferred&apos;)
      {
          var r=confirm(&quot;Are you sure you want to transfer it!&quot;);
          if (r==true)
          {
             window.location.href = &quot;/apex/PublicBookingTransfer?id={!Special_Event_Booking__c.Id}&quot;;
           }
           else{
              alert(&quot;You have cancelled the request to transfer!&quot;);
           }
      }
      else
      {
         alert(&quot;Go through the Public Transfer process first!&quot;);
      }
   }
}</url>
