    <validationRules>
        <fullName>Chk_Overbooking_with_Confirmed_Status_N</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISNEW(),
ISPICKVAL(Status__c, &quot;Confirmed&quot;),
Special_Event__r.No_of_Delegates_Booked__c&gt;= Special_Event__r.Maximum_Places__c,
NOT(Special_Event__r.Allow_Overbooking__c )
)</errorConditionFormula>
        <errorDisplayField>Status__c</errorDisplayField>
        <errorMessage>Event does not allow overbooking. Number of confirmed bookings already equal to or greater than maximum places.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Chk_Overbooking_with_Confirmed_Status_U</fullName>
        <active>true</active>
        <errorConditionFormula>AND(NOT(ISNEW()), 
ISPICKVAL(Status__c, &quot;Confirmed&quot;), 
NOT(ISPICKVAL(PRIORVALUE( Status__c), &quot;Confirmed&quot;)),
Special_Event__r.No_of_Delegates_Booked__c&gt;= Special_Event__r.Maximum_Places__c,
 NOT(Special_Event__r.Allow_Overbooking__c ) 
)</errorConditionFormula>
        <errorDisplayField>Status__c</errorDisplayField>
        <errorMessage>Event does not allow overbooking. Number of confirmed bookings already equal to or greater than maximum places.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Stop_field_changes_all_finance_statuses</fullName>
        <active>false</active>
        <description>If the Finance status is either Invoiced, Paid, Part Paid then some opp fields can&apos;t be changed.</description>
        <errorConditionFormula>IF(
   AND(
      OR(
         ISPICKVAL(Opportunity__r.StageName, &apos;Invoiced&apos;),
         ISPICKVAL(Opportunity__r.StageName, &apos;Paid&apos;), 
         ISPICKVAL(Opportunity__r.StageName, &apos;Part Paid&apos;),
         ISPICKVAL(Opportunity__r.StageName, &apos;Part Invoiced&apos;)
      ),
      ISCHANGED(Booking_Fee__c),
      Opportunity__r.Using_one_to_one_invoicing__c = &apos;Yes&apos;
   ),
   true,
   false
)</errorConditionFormula>
        <errorDisplayField>Booking_Fee__c</errorDisplayField>
        <errorMessage>You cannot change the booking fee when the opportunity has been Invoiced, Part Paid or Paid.</errorMessage>
